{
  "name": "oneplace",
  "version": "0.2.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint-unused": "eslint .",
    "start": "NODE_PATH=./common node .",
    "client": "cross-env NODE_PATH=./common NODE_ENV=production node client/server",
    "client:dev": "cross-env NODE_PATH=./common NODE_ENV=develop node client/server",
    "posttest-unused": "npm run lint && nsp check",
    "test:api": "NODE_PATH=./common ./node_modules/mocha/bin/mocha -b server/test",
    "test:client": "NODE_PATH=./common cross-env BABEL_ENV=test karma start client/test/unit/karma.conf.js --single-run",
    "test": "NODE_PATH=./common npm run test-api && npm run test-client",
    "build": "NODE_PATH=./common rimraf client/dist && npm run build:client && npm run build:server",
    "build:client": "NODE_PATH=./common cross-env NODE_ENV=production webpack --config client/build/webpack.client.config.js --progress --hide-modules",
    "build:server": "NODE_PATH=./common cross-env NODE_ENV=production webpack --config client/build/webpack.server.config.js --progress --hide-modules"
  },
  "dependencies": {
