{
  "name": "account",
  "plural": "accounts",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "mysql": {
      "table": "accounts"
    },
    "validateUpsert": true
  },
  "mixins": {
    "HideRemoteMethods": {
      "methodsVisibility": [
        "byName",
        "follow",
        "getFollowers"
      ]
    }
  },
  "properties": {
    "id": {
      "id": true,
      "type": "number"
    },
    "username": {
      "type": "string"
    },
    "chain": {
      "type": "string"
    }
  },
  "validations": [],
  "acls": [
    {
      "property": "follow",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}